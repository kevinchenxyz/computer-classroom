{
  "name": "computer-classroom",
  "productName": "computer_classroom",
  "description": "controller computer",
  "version": "0.0.3",
  "author": "wininfo<steps>",
  "copyright": "Â© 2017, Gumby inc.",
  "homepage": "http://example.com",
  "license": "MIT",
  "main": "app/background.js",
  "build": {
    "appId": "com.example.computer-classroom",
    "files": [
      "app/**/*",
      "node_modules/**/*",
      "package.json"
    ],
    "publish": [
      {
        "provider": "generic",
        "url": "http://192.168.1.138:8080/agent/"
      }
    ],
    "nsis": {
      "perMachine": true,
      "createDesktopShortcut": true
    }
  },
  "scripts": {
    "build-all": "npm run pack && build -mwl",
    "postinstall": "install-app-deps",
    "build": "gulp build",
    "prerelease": "gulp build --env=production",
    "release-32": "build",
    "release": "build --x64",
    "start": "gulp start",
    "pretest": "gulp build-unit --env=test",
    "test": "electron-mocha app/specs.js.autogenerated --renderer --require source-map-support/register",
    "coverage": "npm test -- -R scripts/istanbul-reporter",
    "pree2e": "gulp build-e2e --env=test",
    "e2e": "mocha app/e2e.js.autogenerated --require source-map-support/register"
  },
  "dependencies": {
    "auto-launch": "^5.0.5",
    "electron-config": "^1.0.0",
    "electron-windows-notifications": "^2.1.1",
    "fs-jetpack": "^1.0.0",
    "node-powershell": "^3.3.1",
    "request": "^2.87.0",
    "username": "^3.0.0"
  },
  "devDependencies": {
    "chai": "^4.0.1",
    "electron": "^1.6.11",
    "electron-builder": "^19.11.0",
    "electron-mocha": "^4.0.0",
    "gulp": "^3.9.1",
    "gulp-batch": "^1.0.5",
    "gulp-less": "^3.3.0",
    "gulp-plumber": "^1.1.0",
    "gulp-util": "^3.0.8",
    "gulp-watch": "^4.3.11",
    "istanbul": "^0.4.5",
    "minimist": "^3.0.2",
    "mocha": "^3.2.0",
    "rollup": "^0.43.0",
    "rollup-plugin-istanbul": "^1.1.0",
    "source-map-support": "^0.4.11",
    "spectron": "^3.6.4"
  }
}
